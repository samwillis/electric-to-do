{
  "name": "to-do",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "eslint",
    "copy-wasm-files": "node copy-wasm-files.js",
    "backend:start": "docker compose --env-file backend/compose/.envrc -f backend/compose/docker-compose.yaml up",
    "backend:stop": "docker compose -f backend/compose/docker-compose.yaml down --volumes",
    "electric:start": "node ./backend/startElectric.js",
    "db:migrate": "node ./db/migrate.js",
    "client:generate": "npx electric-sql generate",
    "client:watch": "npx electric-sql generate --watch",
    "format": "prettier --write --loglevel warn ./src"
  },
  "dependencies": {
    "@capacitor/app": "5.0.6",
    "@capacitor/core": "5.2.3",
    "@capacitor/haptics": "5.0.6",
    "@capacitor/keyboard": "5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@databases/pg": "^5.4.1",
    "@ionic/react": "^7.0.0",
    "@ionic/react-router": "^7.0.0",
    "@prisma/client": "4.8.1",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "electric-sql": "next",
    "fs-extra": "^10.0.0",
    "ionicons": "^7.0.0",
    "lodash.debounce": "^4.0.8",
    "prettier": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "wa-sqlite": "rhashimoto/wa-sqlite#master",
    "zod": "<=3.21.1"
  },
  "devDependencies": {
    "@capacitor/cli": "5.2.3",
    "@electric-sql/prisma-generator": "next",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^16.9.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-react": "^4.0.1",
    "cypress": "^12.7.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^22.1.0",
    "prisma": "4.8.1",
    "shelljs": "^0.8.5",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "description": "An Ionic project"
}
